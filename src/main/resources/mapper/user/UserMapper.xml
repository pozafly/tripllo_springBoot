<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pozafly.tripllo.dao.UserDao">

    <select id="getUser" parameterType="String" resultType="com.pozafly.tripllo.model.User">
        select
            id,
            password,
            name,
            email,
            created_at,
            created_by,
            updated_at,
            updated_by
        from user
        where id = #{id}
    </select>

</mapper>